{
	"manifest_version": 3,
	"name": "Element Locator",
	"version": "0.0.1",
	"description": "Generate locators for web elements",
	"icons": {
		"16": "icons/16.png",
		"32": "icons/32.png",
		"64": "icons/64.png",
		"128": "icons/128.png"
	},
	"content_scripts": [{
		"css": ["styles.css"],
		"js": ["content.js"],
		"matches": ["<all_urls>"]
	}],
	"background": {
		"service_worker": "worker.js"
	},
	"action": {
		"default_icon": {
			"16": "icons/16.png",
			"32": "icons/32.png",
			"64": "icons/64.png",
			"128": "icons/128.png"
		}
	}
}